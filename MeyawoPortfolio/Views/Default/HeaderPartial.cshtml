@model List<MeyawoPortfolio.Models.tblFuture>
@foreach (var item in Model)
{
    <header id="home" class="header">
        <div class="overlay"></div>
        <div class="header-content container">
            <h1 class="header-title">
                <span class="up">@item.Header</span>
                <span class="down">@item.NameSurname</span>
            </h1>
            <p class="header-subtitle">@item.Title</p>

            <a href="https://github.com/yucesefa/" class="btn btn-primary">Github Repolarım</a>
        </div>
    </header>
}
